<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <name name="mapping" ignorecase="false"/>
        <resolver mime="text/x-wm"/>
    </file>
    <file>
        <ext name="wm"/>
        <ext name="mapping"/>
        <resolver mime="text/x-wm"/>
    </file>
</MIME-resolver>
